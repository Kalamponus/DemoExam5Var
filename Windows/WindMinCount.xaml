<Window x:Class="DemoExam5Var.WindMinCount"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DemoExam5Var"
        mc:Ignorable="d"
        Title="Изменение количества" Height="150" Width="350">
    <Grid>
        <StackPanel>
            <GroupBox Header="Новое минимальное количество">
                <TextBox x:Name="tbNewMinCount" PreviewTextInput="tb_PreviewTextInput"/>
            </GroupBox>
            <Button x:Name="btnSave" Content="Изменить" Click="btnSave_Click"/>
            <Button x:Name="btnBack" Content="Отмена" Click="btnBack_Click" Margin="0 10"/>
        </StackPanel>
    </Grid>
</Window>
